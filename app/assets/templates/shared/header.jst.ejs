<a href="/#/shops">
  <img src="http://i.imgur.com/hLCTHtU.png" class="logo">
</a>

<div class="user-info">
<% if(ShopUp.currentUser.isSignedIn()) { %>

  <p>
    Signed in as:
    <a href="#users/<%= ShopUp.currentUser.id %>"><%= ShopUp.currentUser.escape("fname") %></a> |
    <a href="#" id="sign-out-link">Sign Out</a>
  </p>

<% } else { %>

  <p>
    <a href="#/signup">Sign Up</a> |
    <a href="#/login">Log In</a>
  </p>

<% } %>
</div>
